{
  "AllowedHosts": "*",
  "App": {
    "Name": "Mochi",
    "Environment": "Env",
    "Version": "1.0.0",
    "ApiUrl": "https://localhost:7010/"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=.\\;Initial Catalog=Mochi;Integrated Security=SSPI"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}",
          "path": "C:\\\\WebAppsLogs\\Mochi\\app.log",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
